msgid "Log In"
msgstr "כניסה"

#: src/user-list.vala:828
#, c-format
msgid "Login as %s"
msgstr "כניסה בתור %s"

#: src/user-list.vala:832
msgid "Retry"
msgstr "ניסיון חוזר"

#: src/user-list.vala:833
#, c-format
